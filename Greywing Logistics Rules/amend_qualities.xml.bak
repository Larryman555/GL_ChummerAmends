<chummer xmlns="" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/XMLSchema qualities.xsd">

  <!-- Quality Tweaks -->
  <qualities>
	<!-- Spirit Hunter Karma change-->
	<quality>
	  <name>Spirit Hunter I</name>
	  <karma>10</karma>
	</quality>
	<quality>
	  <name>Spirit Hunter II</name>
	  <karma>10</karma>
	</quality>
	<quality>
	  <name>Spirit Hunter III</name>
	  <karma>10</karma>
	</quality>
    <!-- Lets non-magicians take Witness My Hate-->
    <quality>
      <name>Witness My Hate</name>
      <required>
        <oneof amendoperation="replace">
          <magenabled />
        </oneof>
      </required>
    </quality>

    <!-- Make Outdoorsman not show up in a funky way-->
	<quality>
		<name>Focused Concentration</name>
		<karma>3</karma>
	</quality>
    <quality>
      <name>Outdoorsman</name>
      <bonus amendoperation="replace">
        <specificskill>
          <name>Perception</name>
          <bonus>-1</bonus>
          <condition>Urban</condition>
        </specificskill>
        <specificskill>
          <name>Survival</name>
          <bonus>-1</bonus>
          <condition>Urban</condition>
        </specificskill>
      </bonus>
      <forbidden amendoperation="append">
        <oneof>
          <metatype>AI</metatype>
        </oneof>
      </forbidden>
    </quality>    

    <!-- Make City Slicker not show up in a funky way -->
    <quality>
      <name>City Slicker</name>
      <bonus amendoperation="replace">
        <skillgroup>
          <name>Outdoors</name>
          <bonus>1</bonus>
          <condition>Urban</condition>
        </skillgroup>
        <specificskill>
          <name>Perception</name>
          <bonus>-1</bonus>
          <condition>Not Urban</condition>
        </specificskill>
        <specificskill>
          <name>Survival</name>
          <bonus>-1</bonus>
          <condition>Not Urban</condition>
        </specificskill>
      </bonus>
      <forbidden amendoperation="append">
        <oneof>
          <metatype>AI</metatype>
        </oneof>
      </forbidden>
    </quality>    

    <!-- Community Connection for non orks-->
    <quality>
      <name>Community Connection</name>
      <required amendoperation="replace"></required>
    </quality>
  </qualities>

